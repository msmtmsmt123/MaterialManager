<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.Cabinet" parent="@style/CabinetParent" />

    <style name="Theme.CabinetDark" parent="@style/CabinetDarkParent" />

    <style name="Theme.CabinetTrueBlack" parent="@style/CabinetTrueBlackParent" />


    <style name="ButtonCabinet" parent="android:Widget.Holo.Light.Button">
        <item name="android:background">@drawable/rect_button_selector</item>
        <item name="android:textColor">#fff</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:minWidth">64dp</item>
    </style>

    <style name="ButtonCabinetDark" parent="android:Widget.Holo.Button">
        <item name="android:background">@drawable/rect_button_selector_dark</item>
        <item name="android:textColor">#fff</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:minWidth">64dp</item>
    </style>

    <style name="_FileMenuParent">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:src">?overflow</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:background">?menu_selector</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginRight">23dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:focusableInTouchMode">false</item>
        <item name="android:focusable">false</item>
    </style>

    <style name="FileMenu" parent="@style/_FileMenuParent" />

</resources>